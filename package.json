{
  "name": "drawer_editor",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "scripts": {
    "build:docs": "rspress build",
    "change": "changeset",
    "changeset": "changeset",
    "check-spell": "pnpm cspell && heading-case",
    "commitlint": "commitlint --edit",
    "dev:docs": "rspress dev",
    "format": "prettier . --write && biome check --write && heading-case --write",
    "preinstall": "npx only-allow pnpm",
    "lint": "biome check . --diagnostic-level=warn && prettier . --check && pnpm run check-spell",
    "prepare": "husky",
    "preview:docs": "rspress preview",
    "preview:website": "cd website && npm run preview",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "preuninstall": "npx only-allow pnpm"
  },
  "lint-staged": {
    "*.{md,mdx,css,less,scss,json,jsonc,json5}": "prettier --write",
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "biome check --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@playwright/test": "1.56.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/node": "^22.8.1",
    "check-dependency-version-consistency": "^5.0.1",
    "cross-env": "^10.1.0",
    "cross-spawn": "^7.0.6",
    "cspell": "^9.3.2",
    "cspell-ban-words": "^0.0.4",
    "get-port": "5.1.1",
    "heading-case": "^1.0.3",
    "husky": "^9.1.7",
    "lint-staged": "~15.5.2",
    "nx": "21.6.10",
    "path-serializer": "0.5.1",
    "playwright": "1.56.1",
    "prettier": "3.7.3",
    "prettier-plugin-packagejson": "^2.5.20",
    "rspress": "^1.46.2",
    "tree-kill": "^1.2.2",
    "vitest": "2.1.9",
    "zx": "^8.8.5"
  },
  "packageManager": "pnpm@10.20.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.20.0"
  }
}
